<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Team.
    -->
    <changeSet id="20170816161055-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="teams_id"
                                 baseTableName="team_player"
                                 constraintName="fk_team_player_teams_id"
                                 referencedColumnNames="id"
                                 referencedTableName="team"/>
        <addForeignKeyConstraint baseColumnNames="players_id"
                                 baseTableName="team_player"
                                 constraintName="fk_team_player_players_id"
                                 referencedColumnNames="id"
                                 referencedTableName="player"/>
        
        <addForeignKeyConstraint baseColumnNames="teams_id"
                                 baseTableName="team_team_list"
                                 constraintName="fk_team_team_list_teams_id"
                                 referencedColumnNames="id"
                                 referencedTableName="team"/>
        <addForeignKeyConstraint baseColumnNames="team_lists_id"
                                 baseTableName="team_team_list"
                                 constraintName="fk_team_team_list_team_lists_id"
                                 referencedColumnNames="id"
                                 referencedTableName="team_list"/>
        
    </changeSet>
</databaseChangeLog>
